<% @products.each do |p| %>
<div class="row">
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="..." alt="...">
      <div class="caption">
        <h3><%= p.title %></h3>
        <p><%= p.description %></p>
        <p><%= p.price %></p>
        <p><%= link_to "Go to Product", product_path(p), class: "btn btn-primary" %></p>
      </div>
    </div>
  </div>
</div>
<%end%>
